from Tkinter import *
class Application(Frame):


    def __init__ (self, master):
        Frame.__init__ (self,master)
        self.grid()
        self.create_widgets()
    def create_widgets(self):
#Button1 
    def create_widgets(self):
        self.button1 = Button(self,text="what does this button")
        self.button1.grid()
        

root=Tk()
root.title("GUI")
root.geometry("200x200")
app = Application(root)
root.mainloop()
